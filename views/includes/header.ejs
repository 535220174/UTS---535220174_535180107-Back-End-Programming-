<header>
  <div><a href="/">TE</a></div>
  <nav>
    <ul>
      <li><a href="/login">Login</a></li>
      <li><a href="/signup">Sign up</a></li>
      <% if (locals.isAuth) { %>
        <li>
          <form action="/logout" method="POST">
            <input type="hidden" value="<%= locals.csrfToken %>" name="_csrf">
            <button class="btn">Logout</button>
          </form>
        </li>
      <% } %>
    </ul>
  </nav>
</header>