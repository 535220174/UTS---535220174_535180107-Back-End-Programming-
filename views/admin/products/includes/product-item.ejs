<article class="product-item">
  <img src="<%= product.imageURL %>" alt="Image of <%= product.title %>" />
  <h2><%= product.title %></h2>
  <div class="product-actions">
    <a class="btn" href="products/<%= product.id %>/edit">View & Edit</a>
    <button class="btn btn-alt">Delete</button>
  </div>
</article>
